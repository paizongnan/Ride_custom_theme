      "type": "rich-text",
      "blocks": {
        "heading": { "type": "heading", "settings": { "heading": "MEET THE TEAM", "heading_size": "h0" } },
        "text": {
          "type": "text",
          "settings": {
            "text": "<p>Our team is made up of the world's best competition, lifestyle, and big-wave surfers. We're stoked to have this amazing bunch of ambassadors traveling around the globe supporting us.</p>"
          }
        },
        "button": {
          "type": "button",
